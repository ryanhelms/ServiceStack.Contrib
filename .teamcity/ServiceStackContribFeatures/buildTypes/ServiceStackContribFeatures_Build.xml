<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="e29bb8ee-6cea-4b55-9423-369068aab654" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/9.0/project-config.xsd">
  <name>Build</name>
  <description />
  <settings ref="ServiceStackContrib_Project">
    <options>
      <option name="allowExternalStatus" value="true" />
      <option name="buildNumberPattern" value="0.0.{0}" />
    </options>
    <parameters />
    <build-runners order="RUNNER_3, RUNNER_4, RUNNER_2">
      <runner id="RUNNER_3" name="Get Build Number with short Git Hash" type="jetbrains_powershell">
        <parameters>
          <param name="jetbrains_powershell_bitness" value="x86" />
          <param name="jetbrains_powershell_execution" value="PS1" />
          <param name="jetbrains_powershell_script_code"><![CDATA[$BuildNumber = "%build.number%"
$Hash = "%build.vcs.number%"
$ShortHash = $Hash.substring(0,7)
Write-Host "##teamcity[buildNumber '$BuildNumber.$ShortHash']"]]></param>
          <param name="jetbrains_powershell_script_mode" value="CODE" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_4" name="New build step" type="jb.nuget.installer">
        <parameters>
          <param name="nuget.noCache" value="true" />
          <param name="nuget.path" value="?NuGet.CommandLine.DEFAULT.nupkg" />
          <param name="nuget.updatePackages.mode" value="sln" />
          <param name="nuget.use.restore" value="restore" />
          <param name="nugetCustomPath" value="?NuGet.CommandLine.DEFAULT.nupkg" />
          <param name="nugetPathSelector" value="?NuGet.CommandLine.DEFAULT.nupkg" />
          <param name="sln.path" value="Features/Features.sln" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_2" name="" type="VS.Solution">
        <parameters>
          <param name="build-file-path" value="Features/Features.sln" />
          <param name="msbuild.prop.Configuration" value="Release" />
          <param name="msbuild_version" value="14.0" />
          <param name="octopus_run_octopack" value="true" />
          <param name="run-platform" value="x86" />
          <param name="runnerArgs"><![CDATA[/p:OctoPackPublishPackageToHttp=http://octopus.mobacomm.com:8888/nuget/packages /p:OctoPackPublishApiKey=API-MGF7S5QPTJWXJLAYIYJ8UFP3ZUQ
/p:OctoPackPackageVersion=%system.build.number% 
/p:OctoPackPublishPackagesToTeamCity=true]]></param>
          <param name="targets" value="Rebuild" />
          <param name="teamcity.step.mode" value="default" />
          <param name="toolsVersion" value="14.0" />
          <param name="vs.version" value="vs2015" />
        </parameters>
      </runner>
      <runner id="RUNNER_6" name="" type="octopus.create.release">
        <parameters>
          <param name="octopus_host" value="http://octopus.mobacomm.com:8888/" />
          <param name="octopus_project_name" value="ServiceStack.Contrib.Features" />
          <param name="octopus_releasenumber" value="%build.number%" />
          <param name="octopus_version" value="3.0+" />
          <param name="octopus_waitfordeployments" value="true" />
          <param name="secure:octopus_apikey" value="zxx54d5a53b29fa80766827afd7c507d9db5e0cb51b5709b03bad51cfefd5b61037" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings />
    <requirements />
    <build-triggers>
      <build-trigger id="vcsTrigger" type="vcsTrigger">
        <parameters>
          <param name="branchFilter" value="+:*" />
          <param name="groupCheckinsByCommitter" value="true" />
          <param name="perCheckinTriggering" value="true" />
          <param name="quietPeriodMode" value="DO_NOT_USE" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <build-extensions>
      <extension id="BUILD_EXT_1" type="teamcity.github.status">
        <parameters>
          <param name="github_report_on" value="on start and finish" />
          <param name="guthub_authentication_type" value="password" />
          <param name="guthub_context" value="continuous-integration/teamcity" />
          <param name="guthub_guest" value="true" />
          <param name="guthub_host" value="https://api.github.com" />
          <param name="guthub_owner" value="ryanhelms" />
          <param name="guthub_repo" value="ServiceStack.Contrib" />
          <param name="guthub_username" value="ryanhelms" />
          <param name="secure:guthub_username" value="zxx3bf834275cd363f9775d03cbe80d301b" />
        </parameters>
      </extension>
    </build-extensions>
    <cleanup />
  </settings>
</build-type>

